@{
    Layout = null;
}
<div class="row" id="powerwidgets">
    <div class="col-md-6 bootstrap-grid">
        <!-- New widget -->
        <div class="powerwidget blue powerwidget-sortable" id="registration-form-validation-widget" data-widget-editbutton="false" role="widget" style="">
            <header role="heading">
                <h2>Cadastrar<small>Veículos</small></h2>
                <div class="powerwidget-ctrls" role="menu"><a href="#" class="button-icon powerwidget-fullscreen-btn"><i class="fa fa-arrows-alt "></i></a><a href="#" class="button-icon powerwidget-edit-btn"><i class="fa fa-cog"></i></a> <a href="#" class="button-icon powerwidget-toggle-btn"><i class="fa fa-chevron-circle-up "></i></a></div><span class="powerwidget-loader"></span>
            </header>
            <div class="inner-spacer" role="content">
                <div class="powerwidget-editbox">
                    <div>
                        <label>Estilos:</label>
                        <span data-widget-setstyle="pink" class="pink-btn"></span> <span data-widget-setstyle="blue" class="blue-btn"></span> <span data-widget-setstyle="dark-blue" class="dark-blue-btn"></span> <span data-widget-setstyle="green" class="green-btn"></span> <span data-widget-setstyle="green-alt" class="green-alt-btn"></span> <span data-widget-setstyle="green-acid" class="green-acid-btn"></span> <span data-widget-setstyle="yellow" class="yellow-btn"></span> <span data-widget-setstyle="purple" class="purple-btn"></span> <span data-widget-setstyle="cold-grey" class="cold-grey-btn"></span> <span data-widget-setstyle="dark-cold-grey" class="dark-cold-grey-btn"></span> <span data-widget-setstyle="orange" class="orange-btn"></span> <span data-widget-setstyle="red" class="red-btn"></span> <span data-widget-setstyle="dark-red" class="dark-red-btn"></span> <span data-widget-setstyle="black" class="black-btn"></span>
                    </div>
                </div>
                <form action="" class="orb-form">
                    <header>Novo Veículo</header>
                    <fieldset>
                        <div class="row">
                            <section class="col col-10">
                                <label class="input">
                                    <i class="icon-prepend entypo-briefcase"></i>
                                    <input type="text" name="cliente" placeholder="Cliente">
                                    <b class="tooltip tooltip-bottom-right">Cliente Responsável pelo Veículo</b>
                                </label>
                            </section>
                        </div>
                        <div class="row">
                            <section class="col col-6">
                                <label class="input">
                                    <i class="icon-prepend glyphicon glyphicon glyphicon-import"></i>
                                    <input type="email" name="equipamento" placeholder="Equipamento Associado">
                                    <b class="tooltip tooltip-right">Equipamento Associado ao Veículo</b>
                                </label>
                            </section>
                        </div>
                        <div class="row">
                            <section class="col col-6">
                                <label class="input">
                                    <i class="icon-prepend fa fa-barcode"></i>
                                    <input type="text" name="código" placeholder="Código do Veículo">
                                    <b class="tooltip tooltip-right">Código de identificação</b>
                                </label>
                            </section>
                        </div>
                        <div class="row">
                            <section class="col col-8">
                                <label class="input">
                                    <i class="icon-prepend entypo-doc-text"></i>
                                    <input type="text" name="descricao" placeholder="Descrição">
                                    <b class="tooltip tooltip-right">Descrição do Veículo</b>
                                </label>
                            </section>
                        </div>
                        <div class="row">
                            <section class="col col-8">
                                <label class="input">
                                    <i class="icon-prepend fa fa-male"></i>
                                    <input type="text" name="motorista" placeholder="Motorista Associado">
                                    <b class="tooltip tooltip-right">Motorista Associado ao Veículo</b>
                                </label>
                            </section>
                        </div>
                    </fieldset>
                    <footer>
                        <button type="submit" class="btn btn-primary">Cadastrar</button>
                    </footer>
                </form>
            </div>
        </div>
        <!-- /End Widget -->
    </div>
</div>
